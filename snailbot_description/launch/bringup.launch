<launch>
  <rosparam file="$(find snailbot_description)/config/snailbot.yaml" command="load" />
  <node pkg="rosserial_python" type="serial_node.py" name="motor_controller" output="screen" respawn="true">
    <param name="port" value="/dev/ttyACM0" />
    <param name="baud" value="115200" />
  </node> 
  <!-- Snailbot driver -->
  <node pkg="ros_arduino_base" type="ros_arduino_base_node" name="driver_node" output="screen" respawn="true" />
</launch>
